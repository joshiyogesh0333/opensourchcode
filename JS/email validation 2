  jQuery(document).on('keyup', '#Email', function (e) {
      var inputvalues = jQuery(this).val();
      var regex = /^(([^<>()[\]\\.,;:\s@"]+(\.[^<>()[\]\\.,;:\s@"]+)*)|(".+"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
      if (!regex.test(inputvalues)) {
          return regex.test(inputvalues);
      }
      else {
      }
  });  
